{
   "manifest_version": 2,
   "name": "Roam Snipper",
   "description": "Save selected text to Roam Research, with webpage link.",
   "version": "0.1",
   "permissions": ["activeTab"],
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {},
   "content_scripts": [{
      "matches": ["https://roamresearch.com/*"],
      "js": ["underscore-min.js", "roam_mod.js"]
    }]
}
